  </head>
  <body>
      <div class="container">
      <header>
        <%logo = image_tag("logo.png", :alt => "Sample App", :class => "round") %>
		<%=link_to logo,home_path%>
        <nav class="round">
          <ul>
            <li><%= link_to "Home", '#' %></li>
